@model IEnumerable<Entities.AppUser>

<h2>User list</h2>
<table class="table">
    <thead class="thead-dark">
        <tr>
            <th>
                @Html.DisplayNameFor(model=>model.Email)
            </th>
            <th>
                Access
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Email</td>
                <td>
                    <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-userid="@user.Id">Access</a>
                    @if (user.Email != "admin@gmail.com")
                    {
                        <a class="btn btn-sm btn-primary" asp-action="DeleteUser" asp-controller="Account" asp-route-id="@user.Id">Delete</a>
                    }

                </td>

            </tr>
        }
    </tbody>

</table>